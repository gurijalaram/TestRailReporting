          "type": "string"
        },
        "deploymentType": {
          "type": "string"
        },
        "installationName": {
          "type": "string"
        },
        "applicationIdentity": {
          "type": "string"
        },
        "applicationName": {
          "type": "string"
        },
        "customerMetadata": {
          "type": "object",
          "required": [
            "customerName",
            "maxCadFileSize",
            "salesforceId"
          ],
          "properties": {
