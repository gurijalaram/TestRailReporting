{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "integer"
    },
    "id": {
      "type": "string"
    },
    "priority": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "searchKey": {
      "type": "string"
    },
    "command": {
      "type": "object",
      "properties": {
        "commandType": {
          "type": "string"
        },
        "inputs": {
          "type": "object",
          "properties": {
            "scenarioIterationKey": {
              "type": "object",
              "properties": {
                "scenarioKey": {
                  "type": "object",
                  "properties": {
                    "masterName": {
                      "type": "string"
                    },
                    "stateName": {
                      "type": "string"
                    },
                    "typeName": {
                      "type": "string"
                    },
                    "workspaceId": {
                      "type": "integer"
                    }
                  },
                  "required": [
                    "masterName",
                    "stateName",
                    "typeName",
                    "workspaceId"
                  ]
                },
                "iteration": {
                  "type": "integer"
                }
              },
              "required": [
                "scenarioKey",
                "iteration"
              ]
            },
            "mapping": {
              "type": "object",
              "properties": {
                "level": {
                  "type": "integer"
                },
                "occurrences": {
                  "type": "integer"
                }
              },
              "required": [
                "level",
                "occurrences"
              ]
            }
          },
          "required": [
            "scenarioIterationKey",
            "mapping"
          ]
        },
        "outputs": {
          "type": "object",
          "properties": {
            "scenarioIterationKey": {
              "type": "object",
              "properties": {
                "scenarioKey": {
                  "type": "object",
                  "properties": {
                    "masterName": {
                      "type": "string"
                    },
                    "stateName": {
                      "type": "string"
                    },
                    "typeName": {
                      "type": "string"
                    },
                    "workspaceId": {
                      "type": "integer"
                    }
                  },
                  "required": [
                    "masterName",
                    "stateName",
                    "typeName",
                    "workspaceId"
                  ]
                },
                "iteration": {
                  "type": "integer"
                }
              },
              "required": [
                "scenarioKey",
                "iteration"
              ]
            }
          },
          "required": [
            "scenarioIterationKey"
          ]
        }
      },
      "required": [
        "commandType",
        "inputs",
        "outputs"
      ]
    },
    "dateSubmitted": {
      "type": "string"
    },
    "dateStarted": {
      "type": "string"
    },
    "dateCompleted": {
      "type": "string"
    }
  },
  "required": [
    "version",
    "id",
    "priority",
    "userId",
    "status",
    "searchKey",
    "command",
    "dateSubmitted",
    "dateStarted",
    "dateCompleted"
  ]
}