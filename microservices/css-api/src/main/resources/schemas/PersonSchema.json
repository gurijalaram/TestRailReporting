{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "response": {
      "type": "object",
      "required": [
        "items"
      ],
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "object",
                "required": [
                  "identity",
                  "username",
                  "givenName",
                  "familyName"
                ],
                "properties": {
                  "identity": {
                    "type": "string"
                  },
                  "username": {
                    "type": "string"
                  },
                  "givenName": {
                    "type": "string"
                  },
                  "familyName": {
                    "type": "string"
                  }
                }
              }
            ]
          }
        }
      }
    }
  }
}