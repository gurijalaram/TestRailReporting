{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "supplierViewPermissionIdentity",
    "emailReminder",
    "commentReminder"
  ],
  "optional": [
    "identity"
  ],
  "properties": {
    "identity": {
      "type": "string"
    },
    "supplierViewPermissionIdentity": {
      "type": "string"
    },
    "emailReminder": {
      "type": "object",
      "required": [
        "active",
        "startDuration",
        "frequencyValue"
      ],
      "properties": {
        "active": {
          "type": "boolean"
        },
        "startDuration": {
          "type": "string"
        },
        "frequencyValue": {
          "type": "string"
        }
      }
    },
    "commentReminder": {
      "type": "object",
      "required": [
        "active",
        "startDuration",
        "frequencyValue"
      ],
      "properties": {
        "active": {
          "type": "boolean"
        },
        "startDuration": {
          "type": "string"
        },
        "frequencyValue": {
          "type": "string"
        }
      }
    }
  }
}