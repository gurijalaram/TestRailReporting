{
  "query": {
    "filter": {
      "or": [
        {
          "and": [
            {"contains": {"property": "lastAction", "value": "CREATE"}},
            {"in": {"property": "scenarioState", "values": ["PROCESSING_FAILED","NOT_COSTED"]}}
          ]
        },
        {
          "and": [
            {"equals": {"property": "componentType", "value": "UNKNOWN"}},
            {"equals": {"property": "scenarioType", "value": "VERIFIED"}}
          ]
        }
      ]
    }
  }
}