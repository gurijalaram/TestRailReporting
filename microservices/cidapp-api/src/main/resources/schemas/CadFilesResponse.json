{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "required": [
        "cadFiles"
      ],
      "properties": {
        "cadFiles": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "object",
                "required": [
                  "filename",
                  "resourceName"
                ],
                "properties": {
                  "filename": {
                    "type": "string"
                  },
                  "resourceName": {
                    "type": "string"
                  }
                }
              }
            ]
          }
        }
      }
    }
  }
}