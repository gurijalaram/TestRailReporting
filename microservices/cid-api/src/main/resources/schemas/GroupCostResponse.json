{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "required": [
        "successes",
        "failures"
      ],
      "properties": {
        "successes": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "object",
                "required": [
                  "identity",
                  "createdAt",
                  "createdBy",
                  "scenarioName",
                  "scenarioState"
                ],
                "properties": {
                  "identity": {
                    "type": "string"
                  },
                  "createdAt": {
                    "type": "string"
                  },
                  "createdBy": {
                    "type": "string"
                  },
                  "updatedAt": {
                    "type": "string"
                  },
                  "updatedBy": {
                    "type": "string"
                  },
                  "createdByName": {
                    "type": "string"
                  },
                  "updatedByName": {
                    "type": "string"
                  },
                  "permissions": {
                    "type": "array",
                    "items": {
                    }
                  },
                  "customerIdentity": {
                    "type": "string"
                  },
                  "scenarioName": {
                    "type": "string"
                  },
                  "scenarioType": {
                    "type": "string"
                  },
                  "scenarioState": {
                    "type": "string"
                  },
                  "previousScenarioState": {
                    "type": "string"
                  },
                  "lastAction": {
                    "type": "string"
                  },
                  "ownedBy": {
                    "type": "string"
                  },
                  "locked": {
                    "type": "boolean"
                  },
                  "published": {
                    "type": "boolean"
                  },
                  "systemLocked": {
                    "type": "boolean"
                  },
                  "ownedByName": {
                    "type": "string"
                  }
                }
              }
            ]
          }
        },
        "failures": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "string"
                  },
                  "componentIdentity": {
                    "type": "string"
                  },
                  "componentName": {
                    "type": "string"
                  },
                  "scenarioIdentity": {
                    "type": "string"
                  },
                  "scenarioName": {
                    "type": "string"
                  }
                }
              }
            ]
          }
        }
      }
    }
  }
}