{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "required": [
    "isSystemUser",
    "identity",
    "createdAt",
    "createdBy",
    "updatedAt",
    "updatedBy",
    "customerIdentity",
    "userProfile",
    "email",
    "username",
    "active",
    "mfaRequired",
    "customAttributes",
    "userType"
  ],
  "properties": {
    "isSystemUser": {
      "type": "boolean"
    },
    "identity": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "createdBy": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    },
    "updatedBy": {
      "type": "string"
    },
    "customerIdentity": {
      "type": "string"
    },
    "userProfile": {
      "type": "object",
      "required": [
        "identity",
        "createdAt",
        "createdBy",
        "givenName",
        "familyName"
      ],
      "properties": {
        "identity": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        },
        "createdBy": {
          "type": "string"
        },
        "givenName": {
          "type": "string"
        },
        "familyName": {
          "type": "string"
        }
      }
    },
    "email": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "mfaRequired": {
      "type": "boolean"
    },
    "customAttributes": {
      "type": "object",
      "required": [
        "workspaceId"
      ],
      "properties": {
        "workspaceId": {
          "type": "integer"
        }
      }
    },
    "userType": {
      "type": "string"
    }
  }
}