{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "required": [
    "isValid",
    "isActive",
    "identity",
    "name",
    "type",
    "accountId",
    "accountSecret"
  ],
  "optional": [
    "licenseRefreshedAt",
    "licenseLimit",
    "licenseUsage",
    "validFrom",
    "validTo"
  ],
  "properties": {
    "isValid": {
      "type": "boolean"
    },
    "isActive": {
      "type": "boolean"
    },
    "identity": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "accountId": {
      "type": "string"
    },
    "accountSecret": {
      "type": "string"
    },
    "licenseLimit": {
      "type": "integer"
    },
    "licenseUsage": {
      "type": "integer"
    },
    "licenseRefreshedAt": {
      "type": "string"
    },
    "validFrom": {
      "type": "string"
    },
    "validTo": {
      "type": "string"
    }
  }
}