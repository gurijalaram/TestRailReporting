{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "deployment"
  ],
  "properties": {
    "deployment": {
      "type": "object",
      "required": [
        "name",
        "description",
        "deploymentType",
        "siteIdentity",
        "active",
        "isDefault",
        "createdBy",
        "apVersion",
        "applications"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "deploymentType": {
          "type": "string"
        },
        "siteIdentity": {
          "type": "string"
        },
        "active": {
          "type": "string"
        },
        "isDefault": {
          "type": "string"
        },
        "createdBy": {
          "type": "string"
        },
        "apVersion": {
          "type": "string"
        },
        "applications": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string"
              }
            ]
          }
        }
      }
    }
  }
}