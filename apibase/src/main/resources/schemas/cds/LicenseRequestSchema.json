{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "license"
  ],
  "properties": {
    "license": {
      "$id": "#/properties/license",
      "type": "object",
      "required": [
        "description",
        "apVersion",
        "createdBy",
        "active",
        "license",
        "licenseTemplate"
      ],
      "properties": {
        "description": {
          "$id": "#/properties/license/properties/description",
          "type": "string"
        },
        "apVersion": {
          "$id": "#/properties/license/properties/apVersion",
          "type": "string"
        },
        "createdBy": {
          "$id": "#/properties/license/properties/createdBy",
          "type": "string"
        },
        "active": {
          "$id": "#/properties/license/properties/active",
          "type": "string"
        },
        "license": {
          "$id": "#/properties/license/properties/license",
          "type": "string"
        },
        "licenseTemplate": {
          "$id": "#/properties/license/properties/licenseTemplate",
          "type": "string"
        }
      }
    }
  }
}