{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "MaterialPart",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isUserPart",
        "isSaved",
        "identity",
        "lineItemIdentity",
        "costingStatus",
        "manufacturerPartNumber",
        "averageCost"
      ],
      "properties": {
        "isUserPart": {
          "$id": "#/properties/response/properties/isUserPart",
          "type": "boolean",
          "title": "The Isuserpart Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isSaved": {
          "$id": "#/properties/response/properties/isSaved",
          "type": "boolean",
          "title": "The Issaved Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "identity": {
          "$id": "#/properties/response/properties/identity",
          "type": "string",
          "title": "The Identity Schema",
          "default": "",
          "examples": [
            "-C7L01LB462J"
          ],
          "pattern": "^(.*)$"
        },
        "lineItemIdentity": {
          "$id": "#/properties/response/properties/lineItemIdentity",
          "type": "string",
          "title": "The Lineitemidentity Schema",
          "default": "",
          "examples": [
            "-3932E8JLG13"
          ],
          "pattern": "^(.*)$"
        },
        "costingStatus": {
          "$id": "#/properties/response/properties/costingStatus",
          "type": "string",
          "title": "The Costingstatus Schema",
          "default": "",
          "examples": [
            "Costed"
          ],
          "pattern": "^(.*)$"
        },
        "manufacturerPartNumber": {
          "$id": "#/properties/response/properties/manufacturerPartNumber",
          "type": "string",
          "title": "The Manufacturerpartnumber Schema",
          "default": "",
          "examples": [
            "BAV99"
          ],
          "pattern": "^(.*)$"
        },
        "averageCost": {
          "$id": "#/properties/averageCost",
          "type": "number",
          "title": "The Averagecost Schema",
          "default": 0.0,
          "examples": [
            1.17
          ]
        }
      }
    }
  }
}