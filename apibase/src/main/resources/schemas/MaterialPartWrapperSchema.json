{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "title": "The Response Schema",
      "properties": {
        "isUserPart": {
          "type": "boolean",
          "title": "The Isuserpart Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isSaved": {
          "type": "boolean",
          "title": "The Issaved Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "identity": {
          "type": "string",
          "title": "The Identity Schema",
          "default": "",
          "examples": [
            "-C7L01LB462J"
          ],
          "pattern": "^(.*)$"
        },
        "lineItemIdentity": {
          "type": "string",
          "title": "The Lineitemidentity Schema",
          "default": "",
          "examples": [
            "-3932E8JLG13"
          ],
          "pattern": "^(.*)$"
        },
        "status": {
          "type": "string",
          "title": "The status Schema",
          "default": "",
          "examples": [
            "Costed"
          ],
          "pattern": "^(.*)$"
        },
        "manufacturerPartNumber": {
          "type": "string",
          "title": "The Manufacturerpartnumber Schema",
          "default": "",
          "examples": [
            "BAV99"
          ],
          "pattern": "^(.*)$"
        },
        "averageCost": {
          "type": "number",
          "title": "The Averagecost Schema",
          "default": 0.0,
          "examples": [
            1.17
          ]
        }
      }
    }
  }
}