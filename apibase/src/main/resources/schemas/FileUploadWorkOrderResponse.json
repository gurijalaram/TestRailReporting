{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "version": {
      "type": "integer"
    },
    "id": {
      "type": "string"
    },
    "priority": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "searchKey": {
      "type": "string"
    },
    "command": {
      "type": "object",
      "required": [
        "commandType",
        "inputs"
      ],
      "additionalProperties": true,
      "properties": {
        "commandType": {
          "type": "string"
        },
        "inputs": {
          "type": "object",
          "required": [
            "scenarioName",
            "fileKey",
            "fileName"
          ],
          "additionalProperties": true,
          "properties": {
            "scenarioName": {
              "type": "string"
            },
            "fileKey": {
              "type": "string"
            },
            "fileName": {
              "type": "string"
            }
          }
        },
        "outputs": {
          "type": "object",
          "required": [
            "scenarioIterationKey"
          ],
          "additionalProperties": true,
          "properties": {
            "scenarioIterationKey": {
              "type": "object",
              "required": [
                "scenarioKey",
                "iteration"
              ],
              "additionalProperties": true,
              "properties": {
                "scenarioKey": {
                  "type": "object",
                  "required": [
                    "stateName",
                    "masterName",
                    "typeName",
                    "workspaceId"
                  ],
                  "additionalProperties": true,
                  "properties": {
                    "stateName": {
                      "type": "string"
                    },
                    "masterName": {
                      "type": "string"
                    },
                    "typeName": {
                      "type": "string"
                    },
                    "workspaceId": {
                      "type": "integer"
                    }
                  }
                },
                "iteration": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    },
    "dateSubmitted": {
      "type": "string"
    },
    "dateStarted": {
      "type": "string"
    },
    "dateCompleted": {
      "type": "string"
    }
  }
}