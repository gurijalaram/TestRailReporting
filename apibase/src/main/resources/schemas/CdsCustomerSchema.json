          "type": "string",
          "pattern": "^(.*)$"
        },
        "createdBy": {
          "type": "string",
          "pattern": "^(.*)$"
        },
        "createdAt": {
          "type": "string",
          "pattern": "^(.*)$"
        },
        "updatedAt": {
          "type": "string"
        },
        "name": {
          "type": "string",
          "pattern": "^(.*)$"
        },
        "cloudReference": {
          "type": "string",
          "pattern": "^(.*)$",
          "optional": true
        },
        "description": {
          "type": "string",
          "pattern": "^(.*)$"
        },
        "emailRegexPatterns": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^(.*)$"
          }
        },
        "active": {
          "type": "boolean"
        },
        "customerType": {
          "type": "string",
          "pattern": "^(.*)$"
        }
      }
    }
  }
