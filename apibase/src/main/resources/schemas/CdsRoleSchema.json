{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "required": [
        "identity",
        "createdBy",
        "createdAt",
        "name",
        "description"
      ],
      "properties": {
        "identity": {
          "$id": "#/properties/response/properties/identity",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "createdBy": {
          "$id": "#/properties/response/properties/createdBy",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "createdAt": {
          "$id": "#/properties/response/properties/createdAt",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/response/properties/name",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "description": {
          "$id": "#/properties/response/properties/description",
          "type": "string",
          "pattern": "^(.*)$"
        }
      }
    }
  }
}