{
  "definitions": {},
  "$schema": "Authorization",
  "$id": "token_authorization",
  "type": "object",
  "title": "Common token authorization response ",
  "required": [
  "access_token",
  "expires_in"
  ],
  "properties": {
        "access_token": {
          "$id": "#/properties/access_token",
          "type": "string",
                 "title": "The Access_token Schema",
                 "default": "",
                 "pattern": "^(.*)$"
        },
        "expires_in": {
           "$id": "#/properties/expires_in",
           "type": "integer",
           "default": 0
        }
       }
     }