{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "scenarioKey",
    "primaryPgName",
    "primaryVpeName",
    "usePrimaryAsDefault",
    "initialized",
    "materialCatalogKeyData",
    "currentPpgIdentifier"
  ],
  "properties": {
    "scenarioKey": {
      "type": "object",
      "required": [
        "stateName",
        "masterName",
        "typeName",
        "workspaceId"
      ],
      "properties": {
        "stateName": {
          "type": "string"
        },
        "masterName": {
          "type": "string"
        },
        "typeName": {
          "type": "string"
        },
        "workspaceId": {
          "type": "integer"
        }
      }
    },
    "primaryPgName": {
      "type": "string"
    },
    "primaryVpeName": {
      "type": "string"
    },
    "usePrimaryAsDefault": {
      "type": "boolean"
    },
    "initialized": {
      "type": "boolean"
    },
    "materialCatalogKeyData": {
      "type": "object",
      "$ref": "ScenarioMaterialCatalogKeyData.json"
    },
    "currentPpgIdentifier": {
      "type": "object",
      "$ref": "ScenarioCurrentProcessGroup.json"
    }
  }
}