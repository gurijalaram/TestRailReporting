{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
        "isFirstPage": {
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "pageNumber": {
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10000
          ]
        },
        "pageItemCount": {
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "totalItemCount": {
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "totalPageCount": {
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "hasNextPage": {
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "hasPreviousPage": {
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "items": {
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "isValid",
              "isActive",
              "identity",
              "name",
              "type",
              "accountId",
              "accountSecret"
            ],
            "properties": {
              "isValid": {
                "type": "boolean",
                "title": "The Isvalid Schema",
                "default": false,
                "examples": [
                  false
                ]
              },
              "isActive": {
                "type": "boolean",
                "title": "The Isactive Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "identity": {
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "2L7HLBEDKIBA"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Apriori"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "type": "string",
                "title": "The Type Schema",
                "default": "",
                "examples": [
                  "Silicon Expert"
                ],
                "pattern": "^(.*)$"
              },
              "accountId": {
                "type": "string",
                "title": "The Accountid Schema",
                "default": "",
                "examples": [
                  "Apriori"
                ],
                "pattern": "^(.*)$"
              },
              "accountSecret": {
                "type": "string",
                "title": "The Accountsecret Schema",
                "default": "",
                "examples": [
                  "1CepyEJx!ol"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}