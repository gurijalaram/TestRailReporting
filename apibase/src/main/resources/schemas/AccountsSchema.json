{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
        "isFirstPage": {
          "$id": "#/properties/response/properties/isFirstPage",
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "$id": "#/properties/response/properties/isLastPage",
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "pageNumber": {
          "$id": "#/properties/response/properties/pageNumber",
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "#/properties/response/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10000
          ]
        },
        "pageItemCount": {
          "$id": "#/properties/response/properties/pageItemCount",
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "totalItemCount": {
          "$id": "#/properties/response/properties/totalItemCount",
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "totalPageCount": {
          "$id": "#/properties/response/properties/totalPageCount",
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "hasNextPage": {
          "$id": "#/properties/response/properties/hasNextPage",
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "hasPreviousPage": {
          "$id": "#/properties/response/properties/hasPreviousPage",
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "items": {
          "$id": "#/properties/response/properties/items",
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "$id": "#/properties/response/properties/items/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "isValid",
              "isActive",
              "identity",
              "name",
              "type",
              "accountId",
              "accountSecret"
            ],
            "properties": {
              "isValid": {
                "$id": "#/properties/response/properties/items/items/properties/isValid",
                "type": "boolean",
                "title": "The Isvalid Schema",
                "default": false,
                "examples": [
                  false
                ]
              },
              "isActive": {
                "$id": "#/properties/response/properties/items/items/properties/isActive",
                "type": "boolean",
                "title": "The Isactive Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "identity": {
                "$id": "#/properties/response/properties/items/items/properties/identity",
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "2L7HLBEDKIBA"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "$id": "#/properties/response/properties/items/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Apriori"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "$id": "#/properties/response/properties/items/items/properties/type",
                "type": "string",
                "title": "The Type Schema",
                "default": "",
                "examples": [
                  "Silicon Expert"
                ],
                "pattern": "^(.*)$"
              },
              "accountId": {
                "$id": "#/properties/response/properties/items/items/properties/accountId",
                "type": "string",
                "title": "The Accountid Schema",
                "default": "",
                "examples": [
                  "Apriori"
                ],
                "pattern": "^(.*)$"
              },
              "accountSecret": {
                "$id": "#/properties/response/properties/items/items/properties/accountSecret",
                "type": "string",
                "title": "The Accountsecret Schema",
                "default": "",
                "examples": [
                  "1CepyEJx!ol"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}