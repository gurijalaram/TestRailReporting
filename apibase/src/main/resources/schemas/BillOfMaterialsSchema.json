{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "results"
      ],
      "properties": {
        "isFirstPage": {
          "$id": "#/properties/response/properties/isFirstPage",
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "$id": "#/properties/response/properties/isLastPage",
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "pageNumber": {
          "$id": "#/properties/response/properties/pageNumber",
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "#/properties/response/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10000
          ]
        },
        "pageItemCount": {
          "$id": "#/properties/response/properties/pageItemCount",
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            36
          ]
        },
        "totalItemCount": {
          "$id": "#/properties/response/properties/totalItemCount",
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            36
          ]
        },
        "totalPageCount": {
          "$id": "#/properties/response/properties/totalPageCount",
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "hasNextPage": {
          "$id": "#/properties/response/properties/hasNextPage",
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "hasPreviousPage": {
          "$id": "#/properties/response/properties/hasPreviousPage",
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "results": {
          "$id": "#/properties/response/properties/results",
          "type": "array",
          "title": "The Results Schema",
          "items": {
            "$id": "#/properties/response/properties/results/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "identity",
              "createdAt",
              "createdBy",
              "name",
              "numberOfLineItemsReadyForExport",
              "totalNumberOfLineItems"
            ],
            "properties": {
              "identity": {
                "$id": "#/properties/response/properties/results/items/properties/identity",
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "-5BM107K09E1"
                ],
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "$id": "#/properties/response/properties/results/items/properties/createdAt",
                "type": "string",
                "title": "The Createdat Schema",
                "default": "",
                "examples": [
                  "2019-05-30T16:08:34Z"
                ],
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "$id": "#/properties/response/properties/results/items/properties/createdBy",
                "type": "string",
                "title": "The Createdby Schema",
                "default": "",
                "examples": [
                  "todo"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "$id": "#/properties/response/properties/results/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "apriori-3-items.csv"
                ],
                "pattern": "^(.*)$"
              },
              "numberOfLineItemsReadyForExport": {
                "$id": "#/properties/response/properties/results/items/properties/numberOfLineItemsReadyForExport",
                "type": "integer",
                "title": "The Numberoflineitemsreadyforexport Schema",
                "default": 0,
                "examples": [
                  3
                ]
              },
              "totalNumberOfLineItems": {
                "$id": "#/properties/response/properties/results/items/properties/totalNumberOfLineItems",
                "type": "integer",
                "title": "The Totalnumberoflineitems Schema",
                "default": 0,
                "examples": [
                  3
                ]
              }
            }
          }
        }
      }
    }
  }
}