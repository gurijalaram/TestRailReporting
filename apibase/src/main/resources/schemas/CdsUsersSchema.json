{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
        "isFirstPage": {
          "$id": "#/properties/response/properties/isFirstPage",
          "type": "boolean"
        },
        "isLastPage": {
          "$id": "#/properties/response/properties/isLastPage",
          "type": "boolean"
        },
        "pageNumber": {
          "$id": "#/properties/response/properties/pageNumber",
          "type": "integer"
        },
        "pageSize": {
          "$id": "#/properties/response/properties/pageSize",
          "type": "integer"
        },
        "pageItemCount": {
          "$id": "#/properties/response/properties/pageItemCount",
          "type": "integer"
        },
        "totalItemCount": {
          "$id": "#/properties/response/properties/totalItemCount",
          "type": "integer"
        },
        "totalPageCount": {
          "$id": "#/properties/response/properties/totalPageCount",
          "type": "integer"
        },
        "hasNextPage": {
          "$id": "#/properties/response/properties/hasNextPage",
          "type": "boolean"
        },
        "hasPreviousPage": {
          "$id": "#/properties/response/properties/hasPreviousPage",
          "type": "boolean"
        },
        "items": {
          "$id": "#/properties/response/properties/items",
          "type": "array",
          "items": {
            "$id": "#/properties/response/properties/items/items",
            "type": "object",
            "required": [
              "identity",
              "createdBy",
              "createdAt",
              "userType",
              "userProfile",
              "email",
              "username",
              "active",
              "sites",
              "customerIdentity"
            ],
            "properties": {
              "identity": {
                "$id": "#/properties/response/properties/items/items/properties/identity",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "$id": "#/properties/response/properties/items/items/properties/createdBy",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "$id": "#/properties/response/properties/items/items/properties/createdAt",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "userType": {
                "$id": "#/properties/response/properties/items/items/properties/userType",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "userProfile": {
                "$id": "#/properties/response/properties/items/items/properties/userProfile",
                "type": "object",
                "required": [
                  "identity",
                  "createdBy",
                  "createdAt",
                  "givenName",
                  "familyName"
                ],
                "properties": {
                  "identity": {
                    "$id": "#/properties/response/properties/items/items/properties/userProfile/properties/identity",
                    "type": "string",
                    "pattern": "^(.*)$"
                  },
                  "createdBy": {
                    "$id": "#/properties/response/properties/items/items/properties/userProfile/properties/createdBy",
                    "type": "string",
                    "pattern": "^(.*)$"
                  },
                  "createdAt": {
                    "$id": "#/properties/response/properties/items/items/properties/userProfile/properties/createdAt",
                    "type": "string",
                    "pattern": "^(.*)$"
                  },
                  "givenName": {
                    "$id": "#/properties/response/properties/items/items/properties/userProfile/properties/givenName",
                    "type": "string",
                    "pattern": "^(.*)$"
                  },
                  "familyName": {
                    "$id": "#/properties/response/properties/items/items/properties/userProfile/properties/familyName",
                    "type": "string",
                    "pattern": "^(.*)$"
                  }
                }
              },
              "email": {
                "$id": "#/properties/response/properties/items/items/properties/email",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "username": {
                "$id": "#/properties/response/properties/items/items/properties/username",
                "type": "string",
                "pattern": "^(.*)$"
              },
              "active": {
                "$id": "#/properties/response/properties/items/items/properties/active",
                "type": "boolean"
              },
              "sites": {
                "$id": "#/properties/response/properties/items/items/properties/sites",
                "type": "array",
                "items": {
                  "$id": "#/properties/response/properties/items/items/properties/sites/items",
                  "type": "object",
                  "required": [
                    "identity",
                    "createdBy",
                    "createdAt",
                    "siteIdentity"
                  ],
                  "properties": {
                    "identity": {
                      "$id": "#/properties/response/properties/items/items/properties/sites/items/properties/identity",
                      "type": "string",
                      "pattern": "^(.*)$"
                    },
                    "createdBy": {
                      "$id": "#/properties/response/properties/items/items/properties/sites/items/properties/createdBy",
                      "type": "string",
                      "pattern": "^(.*)$"
                    },
                    "createdAt": {
                      "$id": "#/properties/response/properties/items/items/properties/sites/items/properties/createdAt",
                      "type": "string",
                      "pattern": "^(.*)$"
                    },
                    "siteIdentity": {
                      "$id": "#/properties/response/properties/items/items/properties/sites/items/properties/siteIdentity",
                      "type": "string",
                      "pattern": "^(.*)$"
                    }
                  }
                }
              },
              "customerIdentity": {
                "$id": "#/properties/response/properties/items/items/properties/customerIdentity",
                "type": "string",
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}