{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "properties": {
        "isFirstPage": {
          "$id": "#/properties/response/properties/isFirstPage",
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "$id": "#/properties/response/properties/isLastPage",
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "pageNumber": {
          "$id": "#/properties/response/properties/pageNumber",
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "#/properties/response/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "pageItemCount": {
          "$id": "#/properties/response/properties/pageItemCount",
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            3
          ]
        },
        "totalItemCount": {
          "$id": "#/properties/response/properties/totalItemCount",
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            3
          ]
        },
        "totalPageCount": {
          "$id": "#/properties/response/properties/totalPageCount",
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "hasNextPage": {
          "$id": "#/properties/response/properties/hasNextPage",
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "hasPreviousPage": {
          "$id": "#/properties/response/properties/hasPreviousPage",
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "items": {
          "$id": "#/properties/response/properties/items",
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "$id": "#/properties/response/properties/items/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "identity",
              "createdAt",
              "createdBy",
              "status",
              "level",
              "customerPartNumber",
              "quantity",
              "manufacturerPartNumber",
              "manufacturer",
              "parts"
            ],
            "properties": {
              "identity": {
                "$id": "#/properties/response/properties/items/items/properties/identity",
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "-8626NLE76L3"
                ],
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "$id": "#/properties/response/properties/items/items/properties/createdAt",
                "type": "string",
                "title": "The Createdat Schema",
                "default": "",
                "examples": [
                  "2020-02-04T12:31:15.000+0000"
                ],
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "$id": "#/properties/response/properties/items/items/properties/createdBy",
                "type": "string",
                "title": "The Createdby Schema",
                "default": "",
                "examples": [
                  "2C0MIFB8E82N"
                ],
                "pattern": "^(.*)$"
              },
              "status": {
                "$id": "#/properties/response/properties/items/items/properties/status",
                "type": "string",
                "title": "The Status Schema",
                "default": "",
                "examples": [
                  "Incomplete"
                ],
                "pattern": "^(.*)$"
              },
              "level": {
                "$id": "#/properties/response/properties/items/items/properties/level",
                "type": "integer",
                "title": "The Level Schema",
                "default": 0,
                "examples": [
                  2
                ]
              },
              "customerPartNumber": {
                "$id": "#/properties/response/properties/items/items/properties/customerPartNumber",
                "type": "string",
                "title": "The Customerpartnumber Schema",
                "default": "",
                "examples": [
                  "ABC123"
                ],
                "pattern": "^(.*)$"
              },
              "quantity": {
                "$id": "#/properties/response/properties/items/items/properties/quantity",
                "type": "integer",
                "title": "The Quantity Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "manufacturerPartNumber": {
                "$id": "#/properties/response/properties/items/items/properties/manufacturerPartNumber",
                "type": "string",
                "title": "The Manufacturerpartnumber Schema",
                "default": "",
                "examples": [
                  "BAV99"
                ],
                "pattern": "^(.*)$"
              },
              "manufacturer": {
                "$id": "#/properties/response/properties/items/items/properties/manufacturer",
                "type": "string",
                "title": "The Manufacturer Schema",
                "default": "",
                "examples": [
                  "NATIONAL/TI"
                ],
                "pattern": "^(.*)$"
              },
              "parts": {
                "$id": "#/properties/response/properties/items/items/properties/parts",
                "type": "array",
                "title": "The Parts Schema",
                "items": {
                  "$id": "#/properties/response/properties/items/items/properties/parts/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "properties": {
                    "isActionRequired": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/isActionRequired",
                      "type": "boolean",
                      "title": "The Isactionrequired Schema",
                      "default": false,
                      "examples": [
                        true
                      ]
                    },
                    "isUserPart": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/isUserPart",
                      "type": "boolean",
                      "title": "The Isuserpart Schema",
                      "default": false,
                      "examples": [
                        false
                      ]
                    },
                    "isSaved": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/isSaved",
                      "type": "boolean",
                      "title": "The Issaved Schema",
                      "default": false,
                      "examples": [
                        false
                      ]
                    },
                    "identity": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/identity",
                      "type": "string",
                      "title": "The Identity Schema",
                      "default": "",
                      "examples": [
                        "-91AFKGL7A65"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "externalId": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/externalId",
                      "type": "string",
                      "title": "The Externalid Schema",
                      "default": "",
                      "examples": [
                        "403629622"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "manufacturerPartNumber": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/manufacturerPartNumber",
                      "type": "string",
                      "title": "The Manufacturerpartnumber Schema",
                      "default": "",
                      "examples": [
                        "BAV99"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "manufacturer": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/manufacturer",
                      "type": "string",
                      "title": "The Manufacturer Schema",
                      "default": "",
                      "examples": [
                        "Texas Instruments"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "classification": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/classification",
                      "type": "string",
                      "title": "The Classification Schema",
                      "default": "",
                      "examples": [
                        "Diodes, Transistors and Thyristors > Rectifiers > Rectifiers"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "description": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/description",
                      "type": "string",
                      "title": "The Description Schema",
                      "default": "",
                      "examples": [
                        "High Conductance Ultra Fast Diode"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "availability": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/availability",
                      "type": "string",
                      "title": "The Availability Schema",
                      "default": "",
                      "examples": [
                        "Obsolete"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "dataSheetUrl": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/dataSheetUrl",
                      "type": "string",
                      "title": "The Datasheeturl Schema",
                      "default": "",
                      "examples": [
                        "http://download.siliconexpert.com/pdfs/2015/4/5/7/34/48/451/txn_/manual/natls22663-1.pdf"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "rohs": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/rohs",
                      "type": "string",
                      "title": "The Rohs Schema",
                      "default": "",
                      "examples": [
                        "Unknown"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "rohsVersion": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/rohsVersion",
                      "type": "string",
                      "title": "The Rohsversion Schema",
                      "default": "",
                      "examples": [
                        "2011/65/EU"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "status": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/status",
                      "type": "string",
                      "title": "The Status Schema",
                      "default": "",
                      "examples": [
                        "Costed"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "type": {
                      "$id": "#/properties/response/properties/items/items/properties/parts/items/properties/type",
                      "type": "string",
                      "title": "The Type Schema",
                      "default": "",
                      "examples": [
                        "PCBA"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}