{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "isValid",
    "isActive",
    "identity",
    "name",
    "type",
    "accountId",
    "accountSecret",
    "licenseLimit",
    "licenseUsage",
    "validFrom",
    "validTo"
  ],
  "properties": {
    "isValid": {
      "$id": "#/properties/isValid",
      "type": "boolean",
      "title": "The Isvalid Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "isActive": {
      "$id": "#/properties/isActive",
      "type": "boolean",
      "title": "The Isactive Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "identity": {
      "$id": "#/properties/identity",
      "type": "string",
      "title": "The Identity Schema",
      "default": "",
      "examples": [
        "2L7HLBEDKIBA"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Apriori"
      ],
      "pattern": "^(.*)$"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "Silicon Expert"
      ],
      "pattern": "^(.*)$"
    },
    "accountId": {
      "$id": "#/properties/accountId",
      "type": "string",
      "title": "The Accountid Schema",
      "default": "",
      "examples": [
        "Apriori"
      ],
      "pattern": "^(.*)$"
    },
    "accountSecret": {
      "$id": "#/properties/accountSecret",
      "type": "string",
      "title": "The Accountsecret Schema",
      "default": "",
      "examples": [
        "1CepyEJx!ol"
      ],
      "pattern": "^(.*)$"
    },
    "licenseLimit": {
      "$id": "#/properties/licenseLimit",
      "type": "integer",
      "title": "The Licenselimit Schema",
      "default": 0,
      "examples": [
        10000
      ]
    },
    "licenseUsage": {
      "$id": "#/properties/licenseUsage",
      "type": "integer",
      "title": "The Licenseusage Schema",
      "default": 0,
      "examples": [
        5942
      ]
    },
    "validFrom": {
      "$id": "#/properties/validFrom",
      "type": "string",
      "title": "The Validfrom Schema",
      "default": "",
      "examples": [
        "18/09/2018"
      ],
      "pattern": "^(.*)$"
    },
    "validTo": {
      "$id": "#/properties/validTo",
      "type": "string",
      "title": "The Validto Schema",
      "default": "",
      "examples": [
        "31/12/2019"
      ],
      "pattern": "^(.*)$"
    }
  }
}