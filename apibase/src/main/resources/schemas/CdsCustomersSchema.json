{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
        "isFirstPage": {
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "pageNumber": {
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "pageItemCount": {
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "totalItemCount": {
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            201
          ]
        },
        "totalPageCount": {
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            21
          ]
        },
        "hasNextPage": {
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "hasPreviousPage": {
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "items": {
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "identity",
              "createdBy",
              "createdAt",
              "name",
              "description",
              "emailRegexPatterns",
              "active",
              "customerType"
            ],
            "properties": {
              "identity": {
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "72KL796DEEN7"
                ],
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "type": "string",
                "title": "The Createdby Schema",
                "default": "",
                "examples": [
                  "2DG62EIC1887"
                ],
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "type": "string",
                "title": "The Createdat Schema",
                "default": "",
                "examples": [
                  "2020-02-24T14:21Z"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "asas"
                ],
                "pattern": "^(.*)$"
              },
              "description": {
                "type": "string",
                "title": "The Description Schema",
                "default": "",
                "examples": [
                  "asas"
                ],
                "pattern": "^(.*)$"
              },
              "salesforceId": {
                "type": "string",
                "title": "The Salesforceid Schema",
                "default": "",
                "examples": [
                  "sasasdsadsadsadsa"
                ],
                "pattern": "^(.*)$"
              },
              "emailRegexPatterns": {
                "type": "array",
                "title": "The Emailregexpatterns Schema",
                "items": {
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "sassa"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "active": {
                "type": "boolean",
                "title": "The Active Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "customerType": {
                "type": "string",
                "title": "The Customertype Schema",
                "default": "",
                "examples": [
                  "ON_PREMISE_ONLY"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}