              "createdBy",
              "createdAt",
              "name",
              "description",
              "emailRegexPatterns",
              "active",
              "customerType"
            ],
            "properties": {
              "identity": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createdBy": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "updatedAt": {
                "type": "string"
              },
              "updatedBy": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "name": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "description": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "salesforceId": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "emailRegexPatterns": {
                "type": "array",
                "items": {
                  "type": "string",
                  "pattern": "^(.*)$"
                }
              },
              "active": {
                "type": "boolean"
              },
              "customerType": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "cloudReference": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "if": {
                "properties": {
                  "updatedAt": {
                    "not": {
                      "type": "null"
                    }
