{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "$id": "#/properties/response",
      "type": "object",
      "title": "The Response Schema",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
        "isFirstPage": {
          "$id": "#/properties/response/properties/isFirstPage",
          "type": "boolean",
          "title": "The Isfirstpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "isLastPage": {
          "$id": "#/properties/response/properties/isLastPage",
          "type": "boolean",
          "title": "The Islastpage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "pageNumber": {
          "$id": "#/properties/response/properties/pageNumber",
          "type": "integer",
          "title": "The Pagenumber Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "#/properties/response/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "pageItemCount": {
          "$id": "#/properties/response/properties/pageItemCount",
          "type": "integer",
          "title": "The Pageitemcount Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "totalItemCount": {
          "$id": "#/properties/response/properties/totalItemCount",
          "type": "integer",
          "title": "The Totalitemcount Schema",
          "default": 0,
          "examples": [
            201
          ]
        },
        "totalPageCount": {
          "$id": "#/properties/response/properties/totalPageCount",
          "type": "integer",
          "title": "The Totalpagecount Schema",
          "default": 0,
          "examples": [
            21
          ]
        },
        "hasNextPage": {
          "$id": "#/properties/response/properties/hasNextPage",
          "type": "boolean",
          "title": "The Hasnextpage Schema",
          "default": false,
          "examples": [
            true
          ]
        },
        "hasPreviousPage": {
          "$id": "#/properties/response/properties/hasPreviousPage",
          "type": "boolean",
          "title": "The Haspreviouspage Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "items": {
          "$id": "#/properties/response/properties/items",
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "$id": "#/properties/response/properties/items/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "identity",
              "createdBy",
              "createdAt",
              "name",
              "description",
              "emailRegexPatterns",
              "active",
              "customerType"
            ],
            "properties": {
              "identity": {
                "$id": "#/properties/response/properties/items/items/properties/identity",
                "type": "string",
                "title": "The Identity Schema",
                "default": "",
                "examples": [
                  "72KL796DEEN7"
                ],
                "pattern": "^(.*)$"
              },
		        "createdBy": {
		          "$id": "#/properties/response/properties/createdBy",
		          "type": "string",
		          "pattern": "^(.*)$"
		        },
              "createdBy": {
                "$id": "#/properties/response/properties/items/items/properties/createdBy",
                "type": "string",
                "title": "The Createdby Schema",
                "default": "",
                "examples": [
                  "2DG62EIC1887"
                ],
                "pattern": "^(.*)$"
              },
              "createdAt": {
                "$id": "#/properties/response/properties/items/items/properties/createdAt",
                "type": "string",
                "title": "The Createdat Schema",
                "default": "",
                "examples": [
                  "2020-02-24T14:21Z"
                ],
                "pattern": "^(.*)$"
              },
              "name": {
                "$id": "#/properties/response/properties/items/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "asas"
                ],
                "pattern": "^(.*)$"
              },
              "description": {
                "$id": "#/properties/response/properties/items/items/properties/description",
                "type": "string",
                "title": "The Description Schema",
                "default": "",
                "examples": [
                  "asas"
                ],
                "pattern": "^(.*)$"
              },
              "salesforceId": {
                "$id": "#/properties/response/properties/items/items/properties/salesforceId",
                "type": "string",
                "title": "The Salesforceid Schema",
                "default": "",
                "examples": [
                  "sasasdsadsadsadsa"
                ],
                "pattern": "^(.*)$"
              },
              "emailRegexPatterns": {
                "$id": "#/properties/response/properties/items/items/properties/emailRegexPatterns",
                "type": "array",
                "title": "The Emailregexpatterns Schema",
                "items": {
                  "$id": "#/properties/response/properties/items/items/properties/emailRegexPatterns/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "sassa"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "active": {
                "$id": "#/properties/response/properties/items/items/properties/active",
                "type": "boolean",
                "title": "The Active Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "customerType": {
                "$id": "#/properties/response/properties/items/items/properties/customerType",
                "type": "string",
                "title": "The Customertype Schema",
                "default": "",
                "examples": [
                  "ON_PREMISE_ONLY"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}