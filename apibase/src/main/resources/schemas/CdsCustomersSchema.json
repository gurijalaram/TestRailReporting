{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "response"
  ],
  "properties": {
    "response": {
      "type": "object",
      "required": [
        "isFirstPage",
        "isLastPage",
        "pageNumber",
        "pageSize",
        "pageItemCount",
        "totalItemCount",
        "totalPageCount",
        "hasNextPage",
        "hasPreviousPage",
        "items"
      ],
      "properties": {
