          "type": "boolean"
        },
        "isLastPage": {
          "type": "boolean"
        },
        "pageNumber": {
          "type": "integer"
        },
        "pageSize": {
          "type": "integer"
        },
        "pageItemCount": {
          "type": "integer"
        },
        "totalItemCount": {
          "type": "integer"
        },
        "totalPageCount": {
          "type": "integer"
        },
        "hasNextPage": {
          "type": "boolean"
        },
        "hasPreviousPage": {
          "type": "boolean"
        },
        "items": {
          "type": "array",
